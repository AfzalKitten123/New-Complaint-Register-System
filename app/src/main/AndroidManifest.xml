<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.echo.complaintregistersystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.CreateComplaint" />
        <activity android:name=".Activities.LoginActivity" />
        <activity android:name=".Activities.MainIndividualFragment" />
        <activity android:name=".Activities.MainInstituteFragment" />
        <activity android:name=".Activities.MainResidentFragment" />
        <activity android:name=".ComplaintInfo" />
        <activity android:name=".RegistrationActivity"></activity>
    </application>

</manifest>
